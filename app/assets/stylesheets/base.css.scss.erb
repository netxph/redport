@font-face {
  font-family: 'Open Sans';
  font-style: normal;
  font-weight: 300;
  src: url('<%= asset_path "OpenSans-Light.eot" %>');
  src: local('Open Sans Light'), local('OpenSans-Light'), url('<%= asset_path "OpenSans-Light.woff" %>') format('woff'), url('<%= asset_path "OpenSans-Light.ttf" %>') format('truetype');
}

@font-face {
  font-family: 'Vegur';
  font-style: normal;
  font-weight: 300;
  src: url('<%= asset_path "vegur-l-webfont.eot" %>');
  src: local('Vegur Light'), local('Vegur-Light'), url('<%= asset_path "vegur-l-webfont.woff" %>') format('woff');
}

h1 {
  font-family: 'Open Sans';
  font-size: 24px;
}
